package kr.green.test;

public class Test01 {
	public static void main(String[] args) {
		int count = 0, cnt = 0;
		
		for (int i = 2; i < 100; i++) { // 2~100까지
			count=0;
			for (int j = 1; j <= i; j++) { // 1~i까지 
				if (i%j==0) { // 나누어 떨어지면 약수이므로 개수 세기
					count++;
				}
			}
			if (count==2) { // 약수의 개수가 2개면 소수
				System.out.print(String.format("%5d", i));
				if (++cnt%5==0) {
					System.out.println();
				}
			}
		}
	}
}
