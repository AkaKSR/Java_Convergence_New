package kr.green.io;

import java.io.FileWriter;
import java.io.IOException;
import java.io.Writer;

public class Ex01 {
	
	public static void main(String[] args) {
		// 1. 변수 준비
		Writer wr = null;
		
		try {
			// 2. 열기
			wr = new FileWriter("test01.txt");
			// 3. 사용
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			// 4. 닫기
			wr.close();
		}
	}
}
