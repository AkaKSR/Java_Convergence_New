package kr.green.io;

import java.io.File;
import java.io.FileWriter;
import java.io.IOException;
import java.io.Writer;

public class Ex01 {
	
	public static void main(String[] args) {
		// 1. 변수 준비
		Writer wr = null;
		
		try {
			wr = new FileWriter(new File("test01.txt"));
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
